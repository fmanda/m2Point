UPDATE C SET C.PRICELIST = B.PRICELIST, C.HARGABELI = B.HARGABELI, C.HARGAJUAL1 = B.HARGAJUAL1, C.HARGAJUAL2 = B.HARGAJUAL2, C.HARGAJUAL3 = B.HARGAJUAL3, C.HARGAJUAL4 = B.HARGAJUAL4, C.MODIFIEDDATE = GETDATE(), C.MODIFIEDBY = A.MODIFIEDBY FROM TPRICEQUOTATION A INNER JOIN TPRICEQUOTATIONITEM B ON A.ID = B.QUOTATION_ID INNER JOIN TITEMUOM C ON B.ITEM_ID = C.ITEM_ID AND B.UOM_ID = C.UOM_ID WHERE A.ID = 1